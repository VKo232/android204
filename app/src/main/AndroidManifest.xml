<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.example.dkalita">

	<uses-permission android:name="android.permission.INTERNET" />
	<application
			android:allowBackup="true"
			android:icon="@drawable/ic_launcher"
			android:label="IISWeb"
			android:theme="@style/AppTheme">

		<activity
				android:name=".iisweb.MainActivity"
				android:label="@string/app_name">

			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>

		</activity>



		<activity
			android:name=".signin.signin.SignInActivity"
			android:label="@string/app_name" />

        <activity
            android:name=".iisweb.CustomerActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".iisweb.AccountStockActivityHandler"
            android:label="@string/app_name" />
		<activity
			android:name=".iisweb.AccountStockActivity"
			android:label="@string/app_name" />

	</application>

</manifest>
